
@model IEnumerable<Library.Models.SearchLibraryData_Result>

@{
    ViewBag.Title = "Search Results";
}

<h2>Search Results</h2>

@if (Model != null && Model.Any())
{
    <table class="table">
        <thead>
            <tr>
                <th>Source</th>
                <th>Result</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var result in Model)
            {
                <tr>
                    <td>@result.Source</td>
                    <td>@result.Result</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>No results found.</p>
}
